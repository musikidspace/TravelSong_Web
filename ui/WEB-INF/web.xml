<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>TravelSong_Web</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
      <servlet-name>login</servlet-name><!-- 自己定义的Servlet名，不和servlet重名就行 -->
      <servlet-class>com/lg/servlet/LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>login</servlet-name><!-- 与<servlet>中一致 -->
      <url-pattern>/login</url-pattern><!-- 访问的地址映射 -->
  </servlet-mapping>
  <servlet>
      <servlet-name>systime</servlet-name>
      <servlet-class>com/lg/servlet/SysTimeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
      <servlet-name>systime</servlet-name>
      <url-pattern>/systime</url-pattern>
  </servlet-mapping>
</web-app>